fn makeClosure(closedValue) {
    fn closure() {
        fn inner() {
            return closedValue;
        }
        // "inner" makes closure here
        // must capture pointer to closedValue
        return inner;
    }
    // "closure" makes closure here 0
    // must capture closedValue
    return closure();
}

let x = makeClosure(10);
let y = x();
