it("Empty block should equal to null", || => {
    assert_eq({ }, null);
});

it("Block should evaluate to the last expression inside", || => {
    assert_eq({ 5 }, 5);
    assert_eq({ 5 - 5 }, 0);

    var foo = 10;
    assert_eq({ foo }, 10);

    fn bar() {
        return 5;
    }

    assert_eq({ bar() }, 5);
});