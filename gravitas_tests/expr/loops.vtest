it("While expression without break should equal to null", || => {
    var while_result = while false { };
    assert_eq(while_result, null);
});

it("While expression with break should equal to the break's value", || => {
    var while_result_five = while true { break 5; };
    assert_eq(while_result_five, 5);

    var while_result_null = while true { break; };
    assert_eq(while_result_null, null);
});